<div class="container login">
  <div class="row">
    <div class="col-center col-sm-3">
      <h1>Login</h1>
      <%= form_tag admin_sessions_path do %>
        <p> 
        <%= label_tag :username %></br>
        <%= text_field_tag :username %>
        </p>
        <p>
        <%= label_tag :password %></br>
        <%= password_field_tag :password %>
        </p>
        <p>
        <%= submit_tag 'Log in' %>
        </p>
      <% end %>
    </div>
  </div>
</div>
